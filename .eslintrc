{
  "ecmaFeatures": {
    "jsx": true,
    "modules": true
  },
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "parser": "babel-eslint",
  "rules": {
    "arrow-body-style": 0,
    "arrow-parens": [
      1,
      "as-needed"
    ],
    "arrow-spacing": 1,
    "camelcase": 1,
    "constructor-super": 2,
    "curly": 2,
    "eol-last": 2,
    "eqeqeq": 1,
    "max-len": [
      1,
      130,
      4
    ],
    "no-alert": 2,
    "no-class-assign": 2,
    "no-confusing-arrow": 0,
    "no-cond-assign": 2,
    "no-console": 2,
    "no-const-assign": 2,
    "no-debugger": 2,
    "no-dupe-args": 2,
    "no-dupe-keys": 2,
    "no-dupe-class-members": 2,
    "no-duplicate-case": 2,
    "no-duplicate-imports": 2,
    "no-empty": 2,
    "no-empty-function": 0,
    "no-empty-pattern": 2,
    "no-ex-assign": 2,
    "no-func-assign": 1,
    "no-inner-declarations": 1,
    "no-invalid-regexp": 2,
    "no-invalid-this": 0,
    "no-irregular-whitespace": 2,
    "no-magic-numbers": [
      2,
      {
        "ignore": [
          -1,
          0,
          1,
          2
        ],
        "ignoreArrayIndexes": true
      }
    ],
    "no-multi-spaces": 2,
    "no-native-reassign": 2,
    "no-negated-in-lhs": 2,
    "no-new-func": 2,
    "no-new-object": 2,
    "no-new-require": 2,
    "no-new-symbol": 2,
    "no-new-wrappers": 2,
    "no-obj-calls": 2,
    "no-proto": 2,
    "no-redeclare": 2,
    "no-script-url": 2,
    "no-sparse-arrays": 1,
    "no-this-before-super": 1,
    "no-underscore-dangle": 0,
    "no-unexpected-multiline": 1,
    "no-unreachable": 2,
    "no-useless-constructor": 1,
    "no-var": 0,
    "no-void": 2,
    "prefer-arrow-callback": 1,
    "prefer-const": 0,
    "prefer-rest-params": 1,
    "prefer-spread": 1,
    "object-shorthand": 0,
    "use-isnan": 2,
    "valid-jsdoc": 2,
    "vars-on-top": 1,
    "wrap-iife": 1,
    "yoda": [
      2,
      "always",
      {
        "onlyEquality": true
      }
    ],
    "react/display-name": 0,
    "react/jsx-boolean-value": [
      2,
      "always"
    ],
    "react/jsx-closing-bracket-location": [
      1,
      "after-props"
    ],
    "react/jsx-curly-spacing": [
      1,
      "never"
    ],
    "react/jsx-equals-spacing": [
      2,
      "never"
    ],
    "react/jsx-handler-names": 0,
    "react/jsx-indent": [
      1,
      4
    ],
    "react/jsx-indent-props": 0,
    "react/jsx-key": 2,
    "react/jsx-max-props-per-line": [
      2,
      {
        "maximum": 1
      }
    ],
    "react/jsx-no-bind": 0,
    "react/jsx-no-duplicate-props": 2,
    "react/jsx-no-literals": 0,
    "react/jsx-no-undef": 2,
    "react/jsx-pascal-case": 2,
    "react/jsx-sort-props": 0,
    "react/jsx-space-before-closing": [
      1,
      "never"
    ],
    "react/jsx-uses-react": 2,
    "react/jsx-uses-vars": 2,
    "react/no-danger": 2,
    "react/no-deprecated": 2,
    "react/no-did-mount-set-state": 2,
    "react/no-did-update-set-state": 2,
    "react/no-direct-mutation-state": 1,
    "react/no-is-mounted": 2,
    "react/no-multi-comp": 1,
    "react/no-unknown-property": 2,
    "react/prefer-es6-class": 2,
    "react/prefer-stateless-function": 0,
    "react/prop-types": [
      2,
      {
        "ignore": [
          "store",
          "dispatch",
          "children"
        ]
      }
    ],
    "react/react-in-jsx-scope": 2,
    "react/self-closing-comp": 2,
    "react/sort-comp": 2,
    "react/sort-prop-types": 0,
    "react/wrap-multilines": 1,
    "quotes": [
      1,
      "single"
    ],
    "space-before-function-paren": [
      1,
      {
        "anonymous": "always",
        "named": "never"
      }
    ],
    "strict": [
      2,
      "global"
    ]
  },
  "plugins": [
    "react"
  ]
}
